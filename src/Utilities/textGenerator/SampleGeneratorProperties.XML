<?xml version ="1.0" encoding ="UTF-8"?>

<TextGeneratorProperties>

    <!-- Set the bundles path to the location of the language properties files -->
    <BundlesPath>
      <element>Utilities.textGenerator.languages/LanguagesBundle</element>
    </BundlesPath>

    <!-- List the codes of the language properties files here -->
 	<LanguagesSupported>
		<element>en</element>
		<element>fr</element>
	</LanguagesSupported>

    <!-- Set the default language here -->
 	<DefaultLanguage>
		<element>en</element>
	</DefaultLanguage>
    
</TextGeneratorProperties>